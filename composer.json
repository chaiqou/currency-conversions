{
    "name": "chaiqou/currency-conversions",
    "description": "This is my package currency-conversions",
    "keywords": [
        "chaiqou",
        "currency-conversions"
    ],
    "homepage": "https://github.com/chaiqou/currency-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikoloz Lomtadze",
            "email": "lomtadzenikusha@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Chaiqou\\CurrencyConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chaiqou\\CurrencyConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
